{
    "name": "holyshared/peridot-file-fixture-plugin",
    "keyword": ["peridot", "peridot-php", "untitest", "plugin", "fixture"],
    "description": "File fixture for peridot",
    "require": {
        "peridot-php/peridot": "~1.15",
        "holyshared/file-fixture": "~0.4"
    },
    "require-dev": {
        "peridot-php/peridot-dot-reporter": "~1.0",
        "cloak/peridot-cloak-plugin": "~1.3",
        "expect/peridot-expect-plugin": "~2.0",
        "phpspec/prophecy": "~1.4",
        "cloak/robo-coveralls-kit": "~1.1",
        "codegyre/robo": "~0.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "holyshared",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "holyshared\\peridot\\": "src"
        }
    },
    "scripts": {
        "coveralls": "vendor/bin/robo coveralls:upload"
    },
    "minimum-stability": "stable"
}
